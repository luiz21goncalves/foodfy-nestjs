{
  "name": "foodfy-nestjs",
  "version": "0.0.0",
  "description": "Aprenda a construir os melhores pratos com receitas criadas por profissionais do mundo inteiro.",
  "author": "Luiz Gon√ßalves<luizhbgoncalves@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write .",
    "start": "nest start",
    "start:dev": "nest start --env-file .env.development --watch",
    "start:debug": "nest start --env-file .env.development --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/platform-express": "11.0.10",
    "joi": "17.13.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@eslint/js": "9.21.0",
    "@nestjs/cli": "11.0.4",
    "@nestjs/schematics": "11.0.1",
    "@nestjs/testing": "11.0.10",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.18",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.5",
    "@types/supertest": "6.0.2",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.4.3",
    "prettier": "3.5.2",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1"
  }
}
